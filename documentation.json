            "type": "component",
            "sourceCode": "import { Component, EventEmitter, Input, Output } from '@angular/core';\n\n@Component({\n  selector: 'app-button',\n  templateUrl: './button.component.html',\n  styleUrls: ['./button.component.scss']\n})\nexport class ButtonComponent{\n\n  @Input()\n  primary = false;\n\n  @Input()\n  size: 'small' | 'medium' | 'large' | 'full' = 'medium';\n\n  @Input()\n  label = 'Button';\n\n  @Input()\n  type: 'button' | 'submit' = 'button';\n\n  @Input() callbackFunction: (args: any) => void;\n\n  @Output() click = new EventEmitter<any>();\n  onClick() {\n    this.click.emit();\n    this.callbackFunction('oláa')\n  }\n\n  public get classes(): string[] {\n    const mode = this.primary ? 'button--primary' : 'button--secondary';\n\n    return ['button', `button--${this.size}`, mode];\n  }\n\n}\n",
